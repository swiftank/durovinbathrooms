<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="PixieMedia\ImageCarousel\Block\Adminhtml\Cgroup\Edit" name="pixiemedia_imagecarousel_cgroup_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="PixieMedia\ImageCarousel\Block\Adminhtml\Cgroup\Edit\Tabs" name="pixiemedia_imagecarousel_cgroup_tabs">
                <block class="PixieMedia\ImageCarousel\Block\Adminhtml\Cgroup\Edit\Tab\Cgroup" name="pixiemedia_imagecarousel_cgroup_edit_tab_cgroup"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">cgroup</argument>
                    <argument name="block" xsi:type="string">pixiemedia_imagecarousel_cgroup_edit_tab_cgroup</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
